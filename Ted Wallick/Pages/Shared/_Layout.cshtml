    <style>

        .bg-company-color {
            background-color: tan;
        }

        /*.nav-item {
            color: black;
        }*/

        .nav-style {
            background-image: url(/images/Corkboard/Brown-Cardboard-Paper-Texture.jpg);
            -webkit-box-shadow: 0 17px 17px 0 rgba(0,0,0,0.15),0 27px 55px 0 rgba(0,0,0,0.3);
            box-shadow: 0 17px 17px 0 rgba(0,0,0,0.15),0 27px 55px 0 rgba(0,0,0,0.3);
            /*border-bottom: 5px solid #9B9B9B;
            border-top: 5px solid #9B9B9B;*/
        }

    
        .button-Paper {
            margin-left:5px;

            /*color: rgb(25, 21, 7);*/
            color:black;
            font-size: 21px;
            line-height: 16px;
            padding:12px;
            border-radius: 10px;
            background-image: url('../images/Corkboard/Old-Paper-Texture-button.jpg');
            box-shadow: rgba(0, 0, 0, 0.67) 5px 5px 11px 0px;
            border: 3px ridge rgb(155, 155, 155);
            display: inline-block;
        }

            .myButton:hover {
                background-image: url('../images/Corkboard/Old-Paper-Texture-button.jpg');
                background: #AF9909;
            }

            .myButton:active {
                background-image: url('../images/Corkboard/Old-Paper-Texture-button.jpg');
                background: #F7D60D;
            }

        a:hover, a:visited, a:link, a:active {
            text-decoration: none;
        }
    </style>

</head>
<body id="top">
